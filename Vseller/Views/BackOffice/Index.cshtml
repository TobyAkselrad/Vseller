<link rel="stylesheet" href="~/Content/animate.css">
<link rel="stylesheet" href="~/Content/style.css">
<link rel="stylesheet" href="~/Content/responsive.css">
@using Vseller.Models


@for (int i = 0; i < ViewBag.Tipos.Count; i++)
{
    <a href="@Url.Action("Producto", "BackOffice", new {Producto=ViewBag.Tipos[i].Desc})">
        <div class="col-lg-4">
            <div class="offer_item">
                <img class="img-fluid" src="~/Content/img/@ViewBag.Tipos[i].Img" alt="">
                <div class="offer_text">
                    @ViewBag.Tipos[i].Desc
                </div>
            </div>
        </div>
    </a>
}


